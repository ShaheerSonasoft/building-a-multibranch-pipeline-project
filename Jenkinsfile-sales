node {
    stage ('Checkout'){
        checkout scm
    }
    stage('Build') {
        if(env.BRANCH_NAME == 'master'){
            bat 'call JenkinsScripts/build.bat'
        }
    }
    stage('Test') {
        if(env.BRANCH_NAME == 'master'){
            bat 'call JenkinsScripts/test.bat'
        }
    }
    stage('Deploy'){
        if(env.TAG_NAME == '*'){
            bat 'call JenkinsScripts/start.bat'
        }
    }
}